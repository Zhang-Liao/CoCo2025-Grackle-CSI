(format TRS)
(fun 0 0)
(fun inv 1)
(fun minus 2)
(fun p 1)
(fun s 1)
(rule (inv 0) 0)
(rule (inv (s x)) (p (inv x)))
(rule (inv (p x)) (s (inv x)))
(rule (minus x 0) x)
(rule (minus x (p y)) (s (minus x y)))
(rule (minus x (s y)) (p (minus x y)))
(rule (minus 0 x) (inv x))
(rule (inv x) (minus 0 x))
(rule (inv (minus x y)) (minus y x))
(rule (s (p x)) x)
(rule (p (s x)) x)
