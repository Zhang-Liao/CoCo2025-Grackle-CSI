(format TRS)
(fun b 0)
(fun f 2)
(fun g 1)
(fun h 1)
(fun k 2)
(fun q 3)
(fun r 2)
(rule (g (f b x)) (g (h (h (f (f (h (k (k x x) x)) (h (k (k x x) x))) (h (k (k x x) x)))))))
(rule (f x b) (h (f (f x x) x)))
(rule (k x y) (q x y y))
(rule (q x y b) (r x y))
(rule (r x b) (f x b))
