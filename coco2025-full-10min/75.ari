(format TRS)
(fun + 2)
(fun - 1)
(fun 0 0)
(rule (+ 0 x) x)
(rule (+ (- x) x) 0)
(rule (+ (+ x y) z) (+ x (+ y z)))
(rule (+ x y) (+ y x))
(rule (- 0) 0)
(rule (- (- x)) x)
(rule (- (+ x y)) (+ (- x) (- y)))
