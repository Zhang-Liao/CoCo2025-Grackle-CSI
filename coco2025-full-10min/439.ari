(format TRS)
(fun * 2)
(fun + 2)
(fun 0 0)
(fun 1 0)
(fun E 1)
(rule (E (+ x y)) (* (E x) (E y)))
(rule (E 0) 1)
(rule (+ x 0) x)
(rule (+ 0 x) x)
(rule (* x 1) x)
(rule (* 1 x) x)
(rule (+ (+ x y) z) (+ x (+ y z)))
(rule (+ x (+ y z)) (+ (+ x y) z))
(rule (+ x y) (+ y x))
(rule (* (* x y) z) (* x (* y z)))
(rule (* x (* y z)) (* (* x y) z))
(rule (* x y) (* y x))
