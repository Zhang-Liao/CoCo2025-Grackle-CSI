; [34] Example 10
(format TRS)
(fun 0 0)
(fun T 0)
(fun c 2)
(fun eq 3)
(fun inc 1)
(fun nats 0)
(fun s 1)
(rule (eq n xs xs) T)
(rule (eq (s n) (c x xs) (c x ys)) (eq n xs ys))
(rule nats (c 0 (inc nats)))
(rule (inc (c x xs)) (c (s x) (inc xs)))
