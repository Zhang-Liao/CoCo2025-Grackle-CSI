(format TRS)
(fun Ap 2)
(fun B 3)
(fun I 0)
(fun K 0)
(fun S 0)
(fun false 0)
(fun true 0)
(rule (Ap (Ap (Ap S x) y) z) (Ap (Ap x z) (Ap y z)))
