(format TRS)
(fun A 0)
(fun A' 0)
(fun B 0)
(fun B' 0)
(fun F 2)
(fun G 2)
(rule (F A A) (G B B))
(rule A A')
(rule (F A' x) (F x x))
(rule (F x A') (F x x))
(rule (G B B) (F A A))
(rule B B')
(rule (G B' x) (G x x))
(rule (G x B') (G x x))
