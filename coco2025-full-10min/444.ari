(format TRS)
(fun + 2)
(fun - 1)
(fun 0 0)
(rule (+ x 0) x)
(rule (+ 0 x) x)
(rule (+ x (- x)) 0)
(rule (+ x (+ (- x) y)) y)
(rule (- (+ x y)) (+ (- x) (- y)))
(rule (- 0) 0)
(rule (- (- x)) x)
(rule (+ (+ x y) z) (+ x (+ y z)))
(rule (+ x (+ y z)) (+ y (+ x z)))
(rule (+ x y) (+ y x))
