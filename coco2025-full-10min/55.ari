(format TRS)
(fun Ap 2)
(fun Dk 0)
(fun E 0)
(fun I 0)
(fun K 0)
(fun S 0)
(rule (Ap (Ap (Ap S x) y) z) (Ap (Ap x z) (Ap y z)))
