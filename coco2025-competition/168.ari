(format TRS)
(fun + 2)
(fun 0 0)
(fun dbl 1)
(fun s 1)
(rule (+ 0 y) y)
(rule (+ x 0) x)
(rule (+ (s x) y) (s (+ x y)))
(rule (+ x (s y)) (+ (s y) x))
(rule (+ x (+ y z)) (+ (+ x y) z))
(rule (+ x y) (+ y x))
(rule (dbl x) (+ x x))
(rule (dbl (+ x y)) (+ (dbl x) (dbl y)))
