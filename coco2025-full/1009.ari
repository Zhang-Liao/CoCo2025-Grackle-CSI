(format TRS)
(fun 0 0)
(fun 1 0)
(fun m 1)
(fun plus 2)
(fun times 2)
(rule (plus x 0) x)
(rule (plus 0 1) 1)
(rule (plus x (plus y 1)) (plus (plus x y) 1))
(rule (times x 0) 0)
(rule (times x 1) x)
(rule (times x (plus y 1)) (plus (times x y) x))
(rule (m 0) 0)
(rule (plus (m 1) 1) 0)
(rule (plus (m (plus x 1)) 1) (m x))
(rule (m (m x)) x)
(rule (plus x (m y)) (m (plus (m x) y)))
(rule (times x (m y)) (m (times x y)))
