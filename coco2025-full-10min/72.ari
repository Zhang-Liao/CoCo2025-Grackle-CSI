(format TRS)
(fun a 0)
(fun b 0)
(fun c 0)
(fun f 1)
(fun g 1)
(fun h 1)
(rule (f (g (g x))) a)
(rule (f (g (h x))) b)
(rule (f (h (g x))) b)
(rule (f (h (h x))) c)
(rule (g x) (h x))
(rule a b)
(rule b c)
